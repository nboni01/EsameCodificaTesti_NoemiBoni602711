                <zone type="handWriting" ulx="58" uly="1103" lrx="1017" lry="1249" xml:id="f1_177" /> 
                <!-- tanto affettuosamente  --> 
                <zone type="handWriting" ulx="41" uly="1254" lrx="968" lry="1361" xml:id="f2_177" />
                <!-- tutti. -->
                <zone type="handWriting" ulx="26" uly="1356" lrx="232" lry="1430" xml:id="f3_177" />
                <!-- tuo Giovannino -->
                <zone type="handWriting" ulx="58" uly="1435" lrx="965" lry="1553" xml:id="f4_177" />
            </surface>
            <surface xml:id="back_img177">
                <graphic url='immagini_cartoline/Retro 177.jpg' width='1052px' height='1658px' />
                <!-- Modena, li 10/11-17. -->
                <zone type="handWriting" ulx="250" uly="4" lrx="813" lry="100" xml:id="bdate_177" />
                <!-- 177 -->
                <zone type="handWriting" ulx="885" uly="6" lrx="1030" lry="112" xml:id="bnum_177" />
                <!-- Olivia mia, -->
                <zone type="handWriting" ulx="44" uly="40" lrx="457" lry="133" xml:id="b1_177" />
                <!-- Non stare in ansia per me, te ne prego. -->
                <zone type="handWriting" ulx="29" uly="110" lrx="1038" lry="216" xml:id="b2_177" />
                <!-- Sai bene che io ho con fervore abbrac- -->
                <zone type="handWriting" ulx="16" uly="177" lrx="1028" lry="306" xml:id="b3_177" />
                <!-- ciata la tua sola ed unica causa e nulla -->
                <zone type="handWriting" ulx="19" uly="274" lrx="1032" lry="370" xml:id="b4_177" />
                <!-- più mi scosterà da tale proposito. -->
                <zone type="handWriting" ulx="18" uly="340" lrx="919" lry="438" xml:id="b5_177" />
                <!-- I ?fumi entusiastici?, e qualunque pensiero -->
                <zone type="handWriting" ulx="22" uly="422" lrx="1049" lry="524" xml:id="b6_177" />
                <!--  che ?possa? comunque turbare la tua _ -->
                <zone type="handWriting" ulx="20" uly="513" lrx="1036" lry="609" xml:id="b7_177" />
                <!-- inquietare la tua anima buona, ?saranno? -->
                <zone type="handWriting" ulx="26" uly="611" lrx="1049" lry="694" xml:id="b8_177" />
                <!-- da me bandita al _ ricordo -->
                <zone type="handWriting" ulx="35" uly="686" lrx="1033" lry="758" xml:id="b9_177" />
                <!-- del nostro purissimo amore, per l’eterna -->
                <zone type="handWriting" ulx="27" uly="764" lrx="1042" lry="841" xml:id="b10_177" />
                <!-- fede del nostro affetto e con la sola -->
                <zone type="handWriting" ulx="23" uly="826" lrx="1024" lry="924" xml:id="b11_177" />
                <!-- visione d’un avvenire felice, adorno -->
                <zone type="handWriting" ulx="48" uly="908" lrx="1044" lry="985" xml:id="b12_177" />
                <!-- delle sfumature più sante e più perfette -->
                <zone type="handWriting" ulx="29" uly="994" lrx="1038" lry="1058" xml:id="b13_177" />
                <!-- _ ?spontanee e rigogliosi dai? -->
                <zone type="handWriting" ulx="32" uly="1051" lrx="1013" lry="1119" xml:id="b14_177" />
                <!-- nostri amori già tante volte provati -->
                <zone type="handWriting" ulx="30" uly="1123" lrx="997" lry="1219" xml:id="b15_177" />
                <!-- dal dolore e dalle disavventure. -->
                <zone type="handWriting" ulx="30" uly="1188" lrx="857" lry="1270" xml:id="b16_177" />
                <!-- Olivia cara, ti penso sempre con crescente -->
                <zone type="handWriting" ulx="40" uly="1257" lrx="1030" lry="1329" xml:id="b17_177" />
                <!-- affetto e con infinita passione. -->
                <zone type="handWriting" ulx="35" uly="1312" lrx="897" lry="1413" xml:id="b18_177" />
                <!-- Vorrei abbandonarmi sulle tue amorose -->
                <zone type="handWriting" ulx="80" uly="1404" lrx="1049" lry="1481" xml:id="b19_177" />
                <!-- braccia, sul tuo cuore palpitante e dirti -->
                <zone type="handWriting" ulx="51" uly="1468" lrx="1045" lry="1540" xml:id="b20_177" />
                <!-- ancora t’amo in eterno. -->
                <zone type="handWriting" ulx="53" uly="1532" lrx="742" lry="1595" xml:id="b21_177" />
                <!-- Un bacio tuo Giovanni -->
                <zone type="handWriting" ulx="46" uly="1588" lrx="1012" lry="1646" xml:id="b22_177" />
